<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-md-6 col-md-offset-3">
        <% @movies.each do |movie| %>
          <%= link_to movie_path(movie) do %>
           <h3><%= movie.title %></h3>
           <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>
